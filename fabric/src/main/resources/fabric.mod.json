{
  "schemaVersion": 1,
  "id": "dsurround",
  "version": "${version}",
  "name": "Dynamic Surroundings",
  "description": "Alters the fabric of Minecraft experience by weaving a tapestry of sound and visual effects",
  "authors": [
    "OreCruncher"
  ],
  "contact": {
    "homepage": "https://github.com/OreCruncher/DynamicSurroundingsFabric",
    "issues": "https://github.com/OreCruncher/DynamicSurroundingsFabric/issues"
  },
  "license": "MIT",
  "icon": "assets/dsurround/icon.png",
  "accessWidener": "dsurround.accesswidener",
  "environment": "client",
  "entrypoints": {
    "client": [
      "org.orecruncher.fabric.FabricMod"
    ],
    "modmenu": [
      "org.orecruncher.fabric.ModConfigMenu"
    ]
  },
  "mixins": [
    "dsurround.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.9",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "architectury": ">=13.0.8",
    "fabric-api": "*"
  },
  "suggests": {
    "cloth-config": ">=13.0.121",
    "yet_another_config_lib_v3": ">=3.3.1+1.20.4"
  },
  "custom": {
    "modmenu": {
      "links": {
        "dsurround.mod.link.sources": "https://github.com/OreCruncher/DynamicSurroundingsFabric",
        "dsurround.mod.link.credits": "https://github.com/OreCruncher/DynamicSurroundingsFabric/blob/main/CREDITS.md",
        "dsurround.mod.link.nashorn": "https://github.com/openjdk/nashorn"
      }
    }
  }
}
